{
  "manifest_version": 3,
  "name": "Focus Booster",
  "version": "1.0",
  "description": "Redirect distracting websites to custom messages.",
  "icons": {
    "16": "images/logo.png",
    "48": "images/logo.png",
    "128": "images/logo.png"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "images/logo.png"
  },
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "webNavigation"
  ]
}